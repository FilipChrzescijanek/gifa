<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.Group?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<GridPane fx:id="root" styleClass="root" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1"
		  fx:controller="pwr.chrzescijanek.filip.gifa.controller.Controller">
	<columnConstraints>
		<ColumnConstraints hgrow="SOMETIMES"/>
	</columnConstraints>
	<rowConstraints>
		<RowConstraints vgrow="NEVER"/>
		<RowConstraints vgrow="ALWAYS"/>
	</rowConstraints>
	<MenuBar fx:id="menuBar">
		<Menu fx:id="fileMenu" text="_File">
			<MenuItem fx:id="fileMenuLoadImages" onAction="#loadImages" text="_Load images" accelerator="Shortcut+L"/>
			<MenuItem fx:id="fileMenuExportToCsv" onAction="#exportToCsv" text="_Export to CSV" accelerator="Shortcut+E"/>
			<MenuItem fx:id="fileMenuExportToPng" onAction="#exportToPng" text="_Save samples" accelerator="Shortcut+S"/>
			<MenuItem fx:id="fileMenuRemoveImage" text="_Remove image" onAction="#deleteImage" accelerator="Shortcut+Delete"/>
			<MenuItem fx:id="fileMenuClear" text="_Clear image list" onAction="#clearImages" accelerator="Shortcut+Alt+Delete"/>
			<MenuItem fx:id="fileMenuExit" onAction="#exit" text="E_xit"/>
		</Menu>
		<Menu fx:id="editMenu" text="_Edit">
			<MenuItem fx:id="editMenuVerticalFlip" text="Flip _vertically" onAction="#flipVertical" accelerator="Shift+V"/>
			<MenuItem fx:id="editMenuHorizontalFlip" text="Flip _horizontally" onAction="#flipHorizontal" accelerator="Shift+H"/>
			<MenuItem fx:id="editMenuRotateLeft" text="Rotate _left by 90°" onAction="#rotateLeft" accelerator="Shift+L"/>
			<MenuItem fx:id="editMenuRotateRight" text="Rotate ri_ght by 90°" onAction="#rotateRight" accelerator="Shift+R"/>
			<MenuItem fx:id="editMenuZoomIn" text="_Zoom in" onAction="#zoomIn" accelerator="Shortcut+Add"/>
			<MenuItem fx:id="editMenuZoomOut" text="Zoom _out" onAction="#zoomOut" accelerator="Shortcut+Subtract"/>
			<MenuItem fx:id="editMenuCreateMode" text="Create mo_de" onAction="#setCreateMode" accelerator="Shortcut+Shift+C"/>
			<MenuItem fx:id="editMenuSelectMode" text="Selec_t mode" onAction="#setSelectMode" accelerator="Shortcut+Shift+T"/>
			<MenuItem fx:id="editMenuDeleteSample" text="Delete _sample" onAction="#deleteSample" accelerator="Shortcut+D"/>
			<MenuItem fx:id="editMenuRestoreCharts" text="_Restore charts" onAction="#refresh" accelerator="F5"/>
			<MenuItem fx:id="editMenuMergeCharts" text="_Merge charts" onAction="#merge" accelerator="F6"/>
			<MenuItem fx:id="editMenuExtractChart" text="_Extract chart" onAction="#shift" accelerator="F7"/>
			<MenuItem fx:id="editMenuRemoveCharts" text="Remove _charts" onAction="#delete" accelerator="F8"/>
			<MenuItem fx:id="editMenuSelectAllFeatures" text="Select _all features" onAction="#selectAllFunctions" accelerator="F9"/>
			<MenuItem fx:id="editMenuDeselectAllFeatures" text="Deselect all _features" onAction="#deselectAllFunctions" accelerator="F10"/>
		</Menu>
		<Menu fx:id="navMenu" text="_Navigate">
			<MenuItem fx:id="navMenuTransform" onAction="#transformTab" text="_Transform" accelerator="Shortcut+Alt+T"/>
			<MenuItem fx:id="navMenuSamples" onAction="#samplesTab" text="_Samples" accelerator="Shortcut+Alt+S"/>
			<MenuItem fx:id="navMenuCharts" onAction="#chartsTab" text="_Charts" accelerator="Shortcut+Alt+C"/>
			<MenuItem fx:id="navMenuChartsBySample" onAction="#chartsBySample" text="Charts by sampl_e" accelerator="Shortcut+Shift+S"/>
			<MenuItem fx:id="navMenuAllCharts" onAction="#allCharts" text="_All charts" accelerator="Shortcut+Shift+A"/>
		</Menu>
		<Menu fx:id="runMenu" text="_Run">
			<MenuItem fx:id="runMenuResultsButton" onAction="#calculateResults" text="_Get results" accelerator="F2"/>
			<MenuItem fx:id="runMenuTransformButton" onAction="#transform" text="_Transform images" accelerator="F3"/>
		</Menu>
		<Menu fx:id="optionsMenu" text="_Options">
			<Menu fx:id="optionsMenuTheme" text="_Theme">
				<RadioMenuItem fx:id="optionsMenuThemeLight" onAction="#applyLightTheme" text="Light" accelerator="Shortcut+Alt+Shift+L">
					<toggleGroup>
						<ToggleGroup fx:id="theme"/>
					</toggleGroup>
				</RadioMenuItem>
				<RadioMenuItem fx:id="optionsMenuThemeDark" onAction="#applyDarkTheme" text="Dark" toggleGroup="$theme"
							   accelerator="Shortcut+Alt+Shift+D"/>
			</Menu>
		</Menu>
		<Menu fx:id="helpMenu" text="_Help">
			<MenuItem fx:id="helpMenuAbout" onAction="#about" text="_About"/>
		</Menu>
	</MenuBar>
	<TabPane GridPane.rowIndex="1" fx:id="mainTabPane" tabClosingPolicy="UNAVAILABLE">
		<Tab fx:id="transformTab" closable="false" text="Transform">
			<GridPane fx:id="transformMainPane">
				<columnConstraints>
					<ColumnConstraints hgrow="SOMETIMES" percentWidth="12.0"/>
					<ColumnConstraints hgrow="SOMETIMES" percentWidth="70.0"/>
					<ColumnConstraints hgrow="SOMETIMES" percentWidth="18.0"/>
				</columnConstraints>
				<rowConstraints>
					<RowConstraints vgrow="SOMETIMES"/>
				</rowConstraints>
				<GridPane fx:id="transformLeftGridPane">
					<padding>
						<Insets left="10" right="10" bottom="30"/>
					</padding>
					<columnConstraints>
						<ColumnConstraints hgrow="SOMETIMES" halignment="CENTER"/>
					</columnConstraints>
					<rowConstraints>
						<RowConstraints valignment="CENTER" percentHeight="40.0" vgrow="SOMETIMES"/>
						<RowConstraints valignment="CENTER" percentHeight="20.0" vgrow="SOMETIMES"/>
						<RowConstraints valignment="CENTER" percentHeight="40.0" vgrow="SOMETIMES"/>
					</rowConstraints>
					<VBox fx:id="transformToolbarVBox" alignment="CENTER">
						<Button fx:id="verticalFlipButton" mnemonicParsing="false" onAction="#flipVertical">
							<graphic>
								<ImageView fitHeight="18.0" fitWidth="18.0" pickOnBounds="true"
										   preserveRatio="true">
									<Image url="@../images/vertical-flip.png"/>
								</ImageView>
							</graphic>
							<VBox.margin>
								<Insets top="15.0"/>
							</VBox.margin>
						</Button>
						<Button fx:id="horizontalFlipButton" mnemonicParsing="false" onAction="#flipHorizontal">
							<graphic>
								<ImageView fitHeight="18.0" fitWidth="18.0" pickOnBounds="true"
										   preserveRatio="true">
									<Image url="@../images/horizontal-flip.png"/>
								</ImageView>
							</graphic>
							<VBox.margin>
								<Insets top="5.0"/>
							</VBox.margin>
						</Button>
						<Button fx:id="rotateLeftButton" mnemonicParsing="false" onAction="#rotateLeft">
							<graphic>
								<ImageView fitHeight="18.0" fitWidth="18.0" pickOnBounds="true"
										   preserveRatio="true">
									<Image url="@../images/undo.png"/>
								</ImageView>
							</graphic>
							<VBox.margin>
								<Insets top="5.0"/>
							</VBox.margin>
						</Button>
						<Button fx:id="rotateRightButton" mnemonicParsing="false" onAction="#rotateRight">
							<graphic>
								<ImageView fitHeight="18.0" fitWidth="18.0" pickOnBounds="true"
										   preserveRatio="true">
									<Image url="@../images/redo.png"/>
								</ImageView>
							</graphic>
							<VBox.margin>
								<Insets top="5.0"/>
							</VBox.margin>
						</Button>
					</VBox>
					<VBox fx:id="interpolationVBox" GridPane.rowIndex="1">
						<RadioButton fx:id="cubicRadioButton" mnemonicParsing="false" selected="true"
									 text="Cubic">
							<toggleGroup>
								<ToggleGroup fx:id="interpolation"/>
							</toggleGroup>
							<VBox.margin>
								<Insets top="5.0" left="20.0"/>
							</VBox.margin>
						</RadioButton>
						<RadioButton fx:id="linearRadioButton" mnemonicParsing="false"
									 text="Linear" toggleGroup="$interpolation">
							<VBox.margin>
								<Insets top="5.0" left="20.0"/>
							</VBox.margin>
						</RadioButton>
						<RadioButton fx:id="nearestRadioButton" mnemonicParsing="false"
									 text="Nearest" toggleGroup="$interpolation">
							<VBox.margin>
								<Insets top="5.0" left="20.0"/>
							</VBox.margin>
						</RadioButton>
					</VBox>
					<GridPane fx:id="transformColorControls" GridPane.rowIndex="2">
						<columnConstraints>
							<ColumnConstraints hgrow="SOMETIMES" halignment="CENTER" percentWidth="60.0"/>
							<ColumnConstraints hgrow="SOMETIMES" halignment="CENTER" percentWidth="40.0"/>
						</columnConstraints>
						<rowConstraints>
							<RowConstraints valignment="CENTER" percentHeight="20.0" vgrow="SOMETIMES"/>
							<RowConstraints valignment="CENTER" percentHeight="20.0" vgrow="SOMETIMES"/>
							<RowConstraints valignment="CENTER" percentHeight="20.0" vgrow="SOMETIMES"/>
							<RowConstraints valignment="CENTER" percentHeight="20.0" vgrow="SOMETIMES"/>
							<RowConstraints valignment="CENTER" percentHeight="20.0" vgrow="SOMETIMES"/>
						</rowConstraints>
						<ColorPicker fx:id="transformTriangleFillColor" maxWidth="40.0"
									 GridPane.columnIndex="1"/>
						<ColorPicker fx:id="transformTriangleStrokeColor" maxWidth="40.0"
									 GridPane.columnIndex="1"
									 GridPane.rowIndex="1"/>
						<ColorPicker fx:id="transformFillColor" maxWidth="40.0"
									 GridPane.columnIndex="1" GridPane.rowIndex="2"/>
						<ColorPicker fx:id="transformStrokeColor" maxWidth="40.0"
									 GridPane.columnIndex="1"
									 GridPane.rowIndex="3"/>
						<ColorPicker fx:id="transformBorderColor" maxWidth="40.0"
									 GridPane.columnIndex="1"
									 GridPane.rowIndex="4"/>
						<Label fx:id="transformTriangleFillLabel" text="Area"/>
						<Label fx:id="transformTriangleStrokeLabel" text="Sides"
							   GridPane.rowIndex="1"/>
						<Label fx:id="transformFillLabel" text="Fill"
							   GridPane.rowIndex="2"/>
						<Label fx:id="transformStrokeLabel" text="Stroke"
							   GridPane.rowIndex="3"/>
						<Label fx:id="transformBorderLabel" text="Border"
							   GridPane.rowIndex="4"/>
					</GridPane>
				</GridPane>
				<BorderPane fx:id="transformBorderPane" GridPane.columnIndex="1">
					<top>
						<HBox fx:id="transformTopHBox" alignment="CENTER">
							<Label fx:id="transformInfo" text="Click to select area. Double-click to open compare view."/>
						</HBox>
					</top>
					<center>
						<ScrollPane fx:id="transformScrollPane">
							<Group fx:id="transformImageViewGroup" autoSizeChildren="false">
								<AnchorPane fx:id="transformImageViewAnchor" centerShape="false">
									<ImageView fx:id="transformImageView" preserveRatio="true" AnchorPane.bottomAnchor="0.0"
											   AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"/>
								</AnchorPane>
							</Group>
						</ScrollPane>
					</center>
					<bottom>
						<GridPane fx:id="transformBottomGrid">
							<columnConstraints>
								<ColumnConstraints hgrow="SOMETIMES" percentWidth="30.0"/>
								<ColumnConstraints hgrow="SOMETIMES" halignment="CENTER" percentWidth="40.0"/>
								<ColumnConstraints hgrow="SOMETIMES" percentWidth="15.0"/>
								<ColumnConstraints hgrow="SOMETIMES" percentWidth="15.0" halignment="RIGHT"/>
							</columnConstraints>
							<rowConstraints>
								<RowConstraints vgrow="SOMETIMES" minHeight="40.0"/>
							</rowConstraints>
							<Label fx:id="transformImageSizeLabel" text=""/>
							<Button fx:id="transformImagesButton" text="Transform" onAction="#transform" prefWidth="80.0" GridPane.columnIndex="1"/>
							<ComboBox fx:id="transformScaleCombo" editable="true" prefWidth="80.0" visibleRowCount="5" GridPane.columnIndex="2"/>
							<Label fx:id="transformMousePositionLabel" text="- : -" GridPane.columnIndex="3"/>
						</GridPane>
					</bottom>
				</BorderPane>
				<GridPane fx:id="transformImageListGrid" alignment="CENTER" vgap="15.0" GridPane.columnIndex="2">
					<columnConstraints>
						<ColumnConstraints halignment="CENTER" hgrow="SOMETIMES"/>
					</columnConstraints>
					<rowConstraints>
						<RowConstraints valignment="CENTER" vgrow="SOMETIMES"/>
						<RowConstraints vgrow="SOMETIMES"/>
					</rowConstraints>
					<padding>
						<Insets bottom="20.0" left="20.0" right="20.0" top="10.0"/>
					</padding>
					<ListView fx:id="transformImageList"/>
					<Label fx:id="transformImageListInfo" textAlignment="CENTER" text="No images loaded yet" wrapText="true"/>
					<HBox fx:id="transformImageListToolbar" alignment="CENTER" GridPane.rowIndex="1">
						<Button fx:id="loadImagesButton" mnemonicParsing="false" onAction="#loadImages">
							<graphic>
								<ImageView fitHeight="21.0" fitWidth="21.0" pickOnBounds="true"
										   preserveRatio="true">
									<Image url="@../images/plus.png"/>
								</ImageView>
							</graphic>
							<HBox.margin>
								<Insets right="5.0"/>
							</HBox.margin>
						</Button>
						<Button fx:id="deleteImageButton" mnemonicParsing="false" onAction="#deleteImage">
							<graphic>
								<ImageView fitHeight="21.0" fitWidth="21.0" pickOnBounds="true"
										   preserveRatio="true">
									<Image url="@../images/minus.png"/>
								</ImageView>
							</graphic>
							<HBox.margin>
								<Insets right="5.0"/>
							</HBox.margin>
						</Button>
						<Button fx:id="clearImagesButton" mnemonicParsing="false" onAction="#clearImages">
							<graphic>
								<ImageView fitHeight="21.0" fitWidth="21.0" pickOnBounds="true"
										   preserveRatio="true">
									<Image url="@../images/trash.png"/>
								</ImageView>
							</graphic>
						</Button>
					</HBox>
				</GridPane>
			</GridPane>
		</Tab>

		<Tab fx:id="samplesTab" closable="false" text="Samples">
			<GridPane fx:id="samplesMainPane">
				<columnConstraints>
					<ColumnConstraints hgrow="SOMETIMES" percentWidth="12.0"/>
					<ColumnConstraints hgrow="SOMETIMES" percentWidth="70.0"/>
					<ColumnConstraints hgrow="SOMETIMES" percentWidth="18.0"/>
				</columnConstraints>
				<rowConstraints>
					<RowConstraints vgrow="SOMETIMES"/>
				</rowConstraints>
				<GridPane fx:id="samplesLeftGridPane">
					<padding>
						<Insets left="10" right="10" bottom="30"/>
					</padding>
					<columnConstraints>
						<ColumnConstraints hgrow="SOMETIMES" halignment="CENTER"/>
					</columnConstraints>
					<rowConstraints>
						<RowConstraints valignment="CENTER" percentHeight="0.0" vgrow="SOMETIMES"/>
						<RowConstraints valignment="CENTER" percentHeight="60.0" vgrow="SOMETIMES"/>
						<RowConstraints valignment="CENTER" percentHeight="40.0" vgrow="SOMETIMES"/>
					</rowConstraints>
					<VBox fx:id="samplesModeVBox" alignment="CENTER" GridPane.rowIndex="1">
						<RadioButton fx:id="createRadioButton" mnemonicParsing="false" selected="true"
									 text="Create">
							<toggleGroup>
								<ToggleGroup fx:id="drawMethod"/>
							</toggleGroup>
							<VBox.margin>
								<Insets top="15.0" left="20.0"/>
							</VBox.margin>
						</RadioButton>
						<RadioButton fx:id="selectRadioButton" mnemonicParsing="false"
									 text="Select" toggleGroup="$drawMethod">
							<VBox.margin>
								<Insets top="5.0" left="20.0"/>
							</VBox.margin>
						</RadioButton>
						<RadioButton fx:id="rotateRadioButton" mnemonicParsing="false"
									 text="Rotate" toggleGroup="$drawMethod">
							<VBox.margin>
								<Insets top="5.0" left="20.0"/>
							</VBox.margin>
						</RadioButton>
						<Button fx:id="deleteSampleButton" mnemonicParsing="false" onAction="#deleteSample">
							<graphic>
								<ImageView fitHeight="21.0" fitWidth="21.0" pickOnBounds="true"
										   preserveRatio="true">
									<Image url="@../images/minus.png"/>
								</ImageView>
							</graphic>
							<VBox.margin>
								<Insets top="40.0"/>
							</VBox.margin>
						</Button>
						<Button fx:id="clearSamplesButton" mnemonicParsing="false" onAction="#clearSamples">
							<graphic>
								<ImageView fitHeight="21.0" fitWidth="21.0" pickOnBounds="true"
										   preserveRatio="true">
									<Image url="@../images/trash.png"/>
								</ImageView>
							</graphic>
							<VBox.margin>
								<Insets top="5.0"/>
							</VBox.margin>
						</Button>
					</VBox>
					<GridPane fx:id="samplesColorControls" GridPane.rowIndex="2">
						<columnConstraints>
							<ColumnConstraints hgrow="SOMETIMES" halignment="CENTER" percentWidth="40.0"/>
							<ColumnConstraints hgrow="SOMETIMES" halignment="CENTER" percentWidth="60.0"/>
						</columnConstraints>
						<rowConstraints>
							<RowConstraints valignment="CENTER" percentHeight="33.3" vgrow="SOMETIMES"/>
							<RowConstraints valignment="CENTER" percentHeight="33.3" vgrow="SOMETIMES"/>
							<RowConstraints valignment="CENTER" percentHeight="33.4" vgrow="SOMETIMES"/>
						</rowConstraints>
						<ColorPicker fx:id="samplesFillColor" maxWidth="40.0"
									 GridPane.columnIndex="1"/>
						<ColorPicker fx:id="samplesStrokeColor" maxWidth="40.0"
									 GridPane.columnIndex="1"
									 GridPane.rowIndex="1"/>
						<ColorPicker fx:id="samplesBorderColor" maxWidth="40.0"
									 GridPane.columnIndex="1"
									 GridPane.rowIndex="2"/>
						<Label fx:id="samplesFillLabel" text="Fill"
						/>
						<Label fx:id="samplesStrokeLabel" text="Stroke"
							   GridPane.rowIndex="1"/>
						<Label fx:id="samplesBorderLabel" text="Border"
							   GridPane.rowIndex="2"/>
					</GridPane>
				</GridPane>
				<BorderPane fx:id="samplesBorderPane" GridPane.columnIndex="1">
					<top>
						<HBox fx:id="samplesTopHBox" alignment="CENTER">
							<Label fx:id="samplesInfo" text="Click to select area. Double-click to open compare view."/>
						</HBox>
					</top>
					<center>
						<ScrollPane fx:id="samplesScrollPane">
							<Group fx:id="samplesImageViewGroup" autoSizeChildren="false">
								<AnchorPane fx:id="samplesImageViewAnchor" centerShape="false">
									<ImageView fx:id="samplesImageView" preserveRatio="true" AnchorPane.bottomAnchor="0.0"
											   AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"/>
								</AnchorPane>
							</Group>
						</ScrollPane>
					</center>
					<bottom>
						<GridPane fx:id="samplesBottomGrid">
							<columnConstraints>
								<ColumnConstraints hgrow="SOMETIMES" percentWidth="70.0"/>
								<ColumnConstraints hgrow="SOMETIMES" percentWidth="15.0"/>
								<ColumnConstraints hgrow="SOMETIMES" percentWidth="15.0" halignment="RIGHT"/>
							</columnConstraints>
							<rowConstraints>
								<RowConstraints vgrow="SOMETIMES" minHeight="40.0"/>
							</rowConstraints>
							<Label fx:id="samplesImageSizeLabel" text=""/>
							<ComboBox fx:id="samplesScaleCombo" editable="true" prefWidth="80.0" visibleRowCount="5" GridPane.columnIndex="1"/>
							<Label fx:id="samplesMousePositionLabel" text="- : -" GridPane.columnIndex="2"/>
						</GridPane>
					</bottom>
				</BorderPane>
				<VBox fx:id="rightVBox" alignment="CENTER" GridPane.columnIndex="2">
					<TabPane fx:id="rightVBoxTabPane" tabClosingPolicy="UNAVAILABLE" VBox.vgrow="SOMETIMES">
						<Tab fx:id="toolboxTab" text="Images">
							<GridPane fx:id="samplesImageListGrid" alignment="CENTER" vgap="15.0">
								<columnConstraints>
									<ColumnConstraints halignment="CENTER" hgrow="SOMETIMES"/>
								</columnConstraints>
								<rowConstraints>
									<RowConstraints valignment="CENTER" vgrow="SOMETIMES"/>
									<RowConstraints vgrow="SOMETIMES"/>
								</rowConstraints>
								<padding>
									<Insets bottom="20.0" left="20.0" right="20.0" top="10.0"/>
								</padding>
								<ListView fx:id="samplesImageList"/>
							</GridPane>
						</Tab>
						<Tab fx:id="featuresTab" text="Features">
							<BorderPane fx:id="featuresBorderPane">
								<center>
									<ScrollPane fx:id="featuresScrollPane" fitToWidth="true">
										<VBox fx:id="featuresVBox" spacing="5.0">
											<padding>
												<Insets bottom="5.0" left="10.0" right="10.0" top="5.0"/>
											</padding>
										</VBox>
									</ScrollPane>
								</center>
								<bottom>
									<HBox fx:id="selectionButtonsHBox" alignment="CENTER" minHeight="40.0"
