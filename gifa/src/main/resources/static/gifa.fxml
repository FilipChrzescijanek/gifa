											<HBox.margin>
												<Insets left="15.0" right="5.0"/>
											</HBox.margin>
										</Button>
									</HBox>
								</bottom>
							</BorderPane>
						</Tab>
					</TabPane>
					<Button fx:id="resultsButton" mnemonicParsing="false" onAction="#calculateResults" text="Get results">
						<VBox.margin>
							<Insets bottom="25.0" top="25.0"/>
						</VBox.margin>
					</Button>
				</VBox>
			</GridPane>
		</Tab>

		<Tab fx:id="chartsTab" closable="false" text="Charts">
			<BorderPane fx:id="chartsMainPane">
				<top>
					<HBox fx:id="chartsGraphsHBox" alignment="CENTER">
						<Label fx:id="chartsGraphsInfo" text="Select multiple charts by clicking on them. Clicking again will remove selection."/>
					</HBox>
				</top>
				<bottom>
					<GridPane fx:id="chartsControls">
						<columnConstraints>
							<ColumnConstraints hgrow="SOMETIMES" percentWidth="12.0" halignment="CENTER"/>
							<ColumnConstraints hgrow="SOMETIMES" percentWidth="6.0" halignment="CENTER"/>
							<ColumnConstraints hgrow="SOMETIMES" percentWidth="12.0" halignment="RIGHT"/>
							<ColumnConstraints hgrow="SOMETIMES" percentWidth="12.0" halignment="CENTER"/>
							<ColumnConstraints hgrow="SOMETIMES" percentWidth="14.0" halignment="CENTER"/>
							<ColumnConstraints hgrow="SOMETIMES" percentWidth="14.0" halignment="CENTER"/>
							<ColumnConstraints hgrow="SOMETIMES" percentWidth="30.0" halignment="CENTER"/>
						</columnConstraints>
						<rowConstraints>
							<RowConstraints vgrow="SOMETIMES" minHeight="40.0"/>
						</rowConstraints>
						<BorderPane.margin>
							<Insets left="50.0"/>
						</BorderPane.margin>
						<Label fx:id="chartsColumnsLabel" alignment="CENTER_RIGHT" text="Max. columns" GridPane.columnIndex="0"/>
						<TextField fx:id="chartsColumnsTextField" alignment="CENTER" text="3" GridPane.columnIndex="1"/>
						<Label fx:id="chartsSampleLabel" alignment="CENTER_RIGHT" text="Sample" GridPane.columnIndex="2"/>
						<ComboBox fx:id="chartsSampleComboBox" editable="false" visibleRowCount="5" GridPane.columnIndex="3"/>
						<RadioButton fx:id="chartsBySampleRadioButton" mnemonicParsing="false" text="Charts" selected="true"
									 GridPane.columnIndex="4">
							<toggleGroup>
								<ToggleGroup fx:id="result"/>
							</toggleGroup>
						</RadioButton>
						<RadioButton fx:id="imagesBySampleRadioButton" mnemonicParsing="false" text="Images"
									 toggleGroup="$result" GridPane.columnIndex="5"/>
						<HBox alignment="CENTER" fx:id="chartsGraphsToolbar" GridPane.columnIndex="6">
							<Button fx:id="chartsRefreshButton" onAction="#refresh" alignment="CENTER">
								<graphic>
									<ImageView fitHeight="21.0" fitWidth="21.0" pickOnBounds="true"
											   preserveRatio="true">
										<Image url="@../images/refresh.png"/>
									</ImageView>
								</graphic>
								<HBox.margin>
									<Insets right="5.0"/>
								</HBox.margin>
							</Button>
							<Button fx:id="chartsMergeButton" onAction="#merge" alignment="CENTER">
								<graphic>
									<ImageView fitHeight="21.0" fitWidth="21.0" pickOnBounds="true"
											   preserveRatio="true">
										<Image url="@../images/merge.png"/>
									</ImageView>
								</graphic>
								<HBox.margin>
									<Insets right="5.0"/>
								</HBox.margin>
							</Button>
							<Button fx:id="chartsShiftButton" onAction="#shift" alignment="CENTER">
								<graphic>
									<ImageView fitHeight="21.0" fitWidth="21.0" pickOnBounds="true"
											   preserveRatio="true">
										<Image url="@../images/shift.png"/>
									</ImageView>
								</graphic>
								<HBox.margin>
									<Insets right="5.0"/>
								</HBox.margin>
							</Button>
							<Button fx:id="chartsDeleteButton" onAction="#delete" alignment="CENTER">
								<graphic>
									<ImageView fitHeight="21.0" fitWidth="21.0" pickOnBounds="true"
											   preserveRatio="true">
										<Image url="@../images/trash.png"/>
									</ImageView>
								</graphic>
							</Button>
						</HBox>
					</GridPane>
				</bottom>
				<center>
					<TabPane fx:id="chartsTabPane" side="RIGHT" tabClosingPolicy="UNAVAILABLE" BorderPane.alignment="CENTER">
						<Tab fx:id="allChartsTab" text="Summary">
							<ScrollPane fx:id="allChartsGridScrollPane" fitToWidth="true">
								<GridPane fx:id="allChartsGrid">
									<rowConstraints>
										<RowConstraints vgrow="SOMETIMES"/>
									</rowConstraints>
									<columnConstraints>
										<ColumnConstraints/>
									</columnConstraints>
								</GridPane>
							</ScrollPane>
						</Tab>
						<Tab fx:id="bySampleTab" text="Samples">
							<GridPane>
								<columnConstraints>
									<ColumnConstraints hgrow="SOMETIMES" percentWidth="100.0"/>
								</columnConstraints>
								<rowConstraints>
									<RowConstraints percentHeight="100.0" vgrow="SOMETIMES"/>
								</rowConstraints>
								<ScrollPane fx:id="chartsBySampleGridScrollPane" fitToWidth="true">
									<GridPane fx:id="chartsBySampleGrid">
										<rowConstraints>
											<RowConstraints vgrow="SOMETIMES"/>
										</rowConstraints>
										<columnConstraints>
											<ColumnConstraints/>
										</columnConstraints>
									</GridPane>
								</ScrollPane>
								<ScrollPane fx:id="imagesBySampleGridScrollPane" fitToWidth="true">
									<GridPane fx:id="imagesBySampleGrid" vgap="10.0" hgap="10.0">
										<rowConstraints>
											<RowConstraints vgrow="SOMETIMES"/>
										</rowConstraints>
										<columnConstraints>
											<ColumnConstraints/>
										</columnConstraints>
									</GridPane>
								</ScrollPane>
							</GridPane>
						</Tab>
					</TabPane>
				</center>
			</BorderPane>
		</Tab>
	</TabPane>
</GridPane>
